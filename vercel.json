{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "ENV_FIREBASE_API_KEY": "@env_firebase_api_key",
    "ENV_FIREBASE_AUTH_DOMAIN": "@env_firebase_auth_domain",
    "ENV_FIREBASE_PROJECT_ID": "@env_firebase_project_id",
    "ENV_FIREBASE_STORAGE_BUCKET": "@env_firebase_storage_bucket",
    "ENV_FIREBASE_MESSAGING_SENDER_ID": "@env_firebase_messaging_sender_id",
    "ENV_FIREBASE_APP_ID": "@env_firebase_app_id",
    "ENV_FIREBASE_MEASUREMENT_ID": "@env_firebase_measurement_id",
    "ADMIN_EMAILS": "@admin_emails"
  }
}